<?xml version="1.0" encoding="utf-8" ?>

<!-- This test resets Roslyn options since perf tests no longer reset them in each StartTarget to prevent unnecessary Roslyn binaries from being loaded on startup -->

<TaoTest xmlns="http://microsoft.com/schemas/VSLanguages/TAO">
  <InitTest>
    <StartTarget />
  </InitTest>

  <ScenarioList>

    <Scenario>
      <ForceGC />
    </Scenario>

  </ScenarioList>

  <CleanupTest>
    <CloseTarget />
  </CleanupTest>
</TaoTest>
